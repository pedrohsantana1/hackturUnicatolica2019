<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".views.fragment.FragmentHomeLogim">

    <de.hdodenhof.circleimageview.CircleImageView
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:src="@drawable/ic_frankenstein"
        app:layout_constraintBottom_toTopOf="@+id/guideline15"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.15" />

    <Button
        android:id="@+id/btnHistoricos"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="bot"
        app:layout_constraintBottom_toBottomOf="@+id/btnExplorar"
        app:layout_constraintEnd_toStartOf="@+id/btnExplorar"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline15" />

    <Button
        android:id="@+id/btnExplorar"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Explorar"
        app:layout_constraintEnd_toStartOf="@+id/btnContatos"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/btnHistoricos"
        app:layout_constraintTop_toTopOf="@+id/guideline15" />

    <Button
        android:id="@+id/btnContatos"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Contatos"
        app:layout_constraintBottom_toBottomOf="@+id/btnExplorar"
        app:layout_constraintEnd_toStartOf="@+id/btConfiguracao"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/btnExplorar"
        app:layout_constraintTop_toTopOf="@+id/guideline15" />

    <Button
        android:id="@+id/btConfiguracao"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Conf"
        app:layout_constraintBottom_toBottomOf="@+id/btnContatos"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/btnContatos"
        app:layout_constraintTop_toTopOf="@+id/guideline15" />

    <FrameLayout
        android:id="@+id/frameLayout"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnExplorar">

    </FrameLayout>

</androidx.constraintlayout.widget.ConstraintLayout>